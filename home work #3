#include <iostream>
using namespace std;

int main() {
	int num;	
	cout << "The program finds the sum of even numbers\n";
	cout << "Enter data: ";
	cin >> num;
	if (num % 1 == 0 && num % 2 == 0 && num != 2 && num != 1)
		cout << num << " - is composite number";
	else
		cout << num << " - is prime number";

	return 0;
}
