#include <iostream>
using namespace std;

int main() {
    srand(time(0));
    int m[11]{0};
    int num;
    for (int i = 0; i < 1000; ++i) {
        num = 2 + rand() % (12 - 2 +1);
        m[num-2] += num;
    }
    for (int i = 0; i < 11; ++i) {
        cout << i+2 << "-" << m[i] << endl;
    }
    return 0;
}
